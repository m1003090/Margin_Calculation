<core:View
		controllerName="sap.ui.demo.myFiori.view.Artdetail"
		xmlns="sap.m"
		xmlns:makit="sap.makit"
		xmlns:core="sap.ui.core" 
		xmlns:mvc="sap.ui.core.mvc" 
		xmlns:viz="sap.viz">
	<Page title="{i18n>ArtikelDetail}"
			class="sapUiFioriObjectPage"
			showNavButton="true"
			navButtonPress="handleNavButtonPress">
		<content>
			<ObjectHeader
					id="graphpageheader"
					title="{LIFNR}"
					number="{path : 'MARGINEURO',
				formatter: 'sap.ui.demo.myFiori.util.Formatter.formatMoney'}"
					numberUnit="EUR"
					markFavorite ="true" 	
					markFlagged="true">
				<attributes>
					<ObjectAttribute text="{NAME}" />
					<ObjectAttribute text="{ARTIKELNAME}" />
				</attributes>
				<firstStatus>
					<ObjectStatus
							icon="{STATUS}" 
							/>
				</firstStatus>
			</ObjectHeader>
			<IconTabBar
					expanded="{device>/isNoPhone}" >
				<items>
					<IconTabFilter id="datevsmargin-Line" icon="sap-icon://line-charts" >
						<viz:ui5.Combination width="100%" height="400px">  
							<viz:title>  
								<viz:ui5.types.Title text='hallo'>  
								</viz:ui5.types.Title>  
							</viz:title>  
							<viz:dataset>  
								<viz:ui5.data.FlattenedDataset data="{GRAPHDATA}">  
									<viz:dimensions>  
										<viz:ui5.data.DimensionDefinition  
												axis="1" name="BUSINESS DATE" value="{path:'BUSINESS_DATE',
											formatter:'sap.ui.demo.myFiori.util.Formatter.date'}">  
										</viz:ui5.data.DimensionDefinition>  
									</viz:dimensions>  
									<viz:measures> 
										<viz:ui5.data.MeasureDefinition  
												name='MARGIN(EUR)' value="{path:'MARGINEURO'}">  
										</viz:ui5.data.MeasureDefinition>  

										<viz:ui5.data.MeasureDefinition  
												name='PURCHASE PRICE(EUR)' value="{path:'ACTUAL_PRICE2'}"> 
										</viz:ui5.data.MeasureDefinition>  



									</viz:measures>  
								</viz:ui5.data.FlattenedDataset>  
							</viz:dataset>  
						</viz:ui5.Combination>  
					</IconTabFilter>
					<IconTabFilter
							id="datevsmargin-Bar"
							icon="sap-icon://bar-chart">
						<viz:ui5.StackedColumn width="100%" height="400px" >  
							<viz:title>  
								<viz:ui5.types.Title text='hallo'>  
								</viz:ui5.types.Title>  
							</viz:title>  
							<viz:dataset>  
								<viz:ui5.data.FlattenedDataset data="{GRAPHDATA}">  
									<viz:dimensions>  
										<viz:ui5.data.DimensionDefinition  
												axis="1" name="SALESDATE" value="{MONTH}">  
										</viz:ui5.data.DimensionDefinition>  
									</viz:dimensions>  
									<viz:measures>  
										<viz:ui5.data.MeasureDefinition  
												name='MARGIN(EUR)'  value="{path:'MARGINEURO'}">  
										</viz:ui5.data.MeasureDefinition>  

										<viz:ui5.data.MeasureDefinition  
												name='PURCHASE PRICE(EUR)' value="{path:'ACTUAL_PRICE2'}"> 
										</viz:ui5.data.MeasureDefinition>


									</viz:measures>  
								</viz:ui5.data.FlattenedDataset>  
							</viz:dataset>  
						</viz:ui5.StackedColumn> 
					</IconTabFilter>
				</items>
			</IconTabBar> 
		</content>
	</Page>
</core:View>