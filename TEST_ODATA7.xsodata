service  { 
"APP_TEST"."MASTERPAGEPRO" as "SUPPLIERINFO" with("NAME","CITY","STREET","POSTZAHL","COUNTRY","LIFNR","MARGINEURO","STATUS") 
navigates ("MASTERDATA" as "MASTERDATA")  aggregates always(SUM of "MARGINEURO");
 
 "APP_TEST"."DETAILPAGEPRO" as "STOREINFO" with ("NAME","LIFNR","STOREID","STORELOCATION","MARGINEURO","STATUS")
navigates ("ARTIKELDATA" as "ARTIKELDATA") aggregates always(SUM of "MARGINEURO");
 
 "APP_TEST"."ARTIKELPAGEPROL" as "ARTIKELINFO" with ("NAME","ARTIKELNAME","LIFNR","STOREID","STORELOCATION","MARGINEURO","ARTIKEL","STATUS","SALES_PRICE","SALES_QUANTITY")
 navigates ("GRAPHDATA" as "GRAPHDATA") aggregates always(SUM of "MARGINEURO");
 
 
 "APP_TEST"."ARTIKELDETAIL" as "ARTIKELPERDAY" with ("NAME","ARTIKELNAME","LIFNR","STOREID","MARGINEURO","ARTIKEL","STORELOCATION",
 "BUSINESS_DATE","SALES_PRICE","ACTUAL_PRICE2","YEAR","MONTH","MARGINEUROPERPC")
aggregates always(SUM of "MARGINEURO",SUM of "MARGINEUROPERPC",AVG of "SALES_PRICE");

 	
association "MASTERDATA" principal "SUPPLIERINFO"("LIFNR") multiplicity "1"
dependent "STOREINFO"("LIFNR") multiplicity "*";

association "ARTIKELDATA" principal "STOREINFO"("LIFNR","STOREID") multiplicity "1"
dependent "ARTIKELINFO"("LIFNR","STOREID") multiplicity "*";

association "GRAPHDATA" principal "ARTIKELINFO"("LIFNR","STOREID","ARTIKEL") multiplicity "1"
dependent "ARTIKELPERDAY"("LIFNR","STOREID","ARTIKEL") multiplicity "*";
}

